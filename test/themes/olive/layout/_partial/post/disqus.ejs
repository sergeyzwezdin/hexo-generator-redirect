<%if(theme.disqus!==null&&theme.disqus!==undefined) { %>
<% var Fullkey= post.path %>
<% var keyArr=Fullkey.split('/') %>
<% var key=keyArr[keyArr.length-3]+'/'+keyArr[keyArr.length-2] %>

<div id="disqus_thread"></div>
<script>
var disqus_config = function () {
this.page.url = " <%=post.permalink  %> ";  // Replace PAGE_URL with your page's canonical URL variable
this.page.identifier = "<%= key %> " ; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
};

(function() { // DON'T EDIT BELOW THIS LINE
var d = document, s = d.createElement('script');
var url='https://'+"<%= theme.disqus %>"+'.disqus.com/embed.js';
s.src = url;

s.setAttribute('data-timestamp', +new Date());
(d.head || d.body).appendChild(s);
})();
</script>
<noscript>
Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a>
</noscript>
<% }%>