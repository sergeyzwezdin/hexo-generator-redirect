@import "iewarn"

.main-header
  width: 100%
  height:5rem
  background-color: #ffffff
  border-bottom: 1px solid #f1f1f1
  position: fixed
  top: 0
  left: 0
  right: 0
  display:-webkit-box
  display:-ms-flexbox
  display:flex
  

.nav
  display:-webkit-box
  display:-ms-flexbox
  display:flex
  -webkit-box-pack:justify
  -ms-flex-pack:justify
  justify-content:space-between


@media mq-mobile
{
.nav-list{
 padding: 0 0.5rem!important;
 font-size: 1.3rem!important;
} 

}
.logo
  margin:0px
.nav-list

  list-style: none
  color: #71777c
  padding: 0 1.5rem
  font-size: 1.5rem
  -ms-flex-pack: start
  -webkit-box-pack: start
  justify-content: flex-start;
  cursor: pointer
  a:hover
      border-bottom:1px solid #007fff
      color:#007fff
  li
    list-style:none
  img
    border-radius:50%;
    padding: 5px;

